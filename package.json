    "@types/node": "~9.4.7",
    "codelyzer": "~4.2.1",
    "concurrently": "^3.5.1",
    "expose-loader": "^0.7.5",
    "json-concat": "0.0.1",
    "json-schema-faker": "^0.5.0-rc9",
    "json-server": "^0.12.1",
    "jsonfile": "^4.0.0",
    "lodash": "^4.17.5",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
