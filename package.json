{
  "name": "researching.app",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "main": "node ./mocks/run.js",
    "start": "ng serve  --port 65000",
    "build": "ng build --prod",
    "mock:server": "npm run concat:mocks && json-server --routes routes.json --watch mocks/data.json",
    "start:proxy": "ng serve   --port 65000 --proxy-config proxy.conf.json",
    "start:proxy:mock:server": "concurrently --kill-others \"npm run mock:server\" \"npm run start:proxy\"",
    "concat:mocks": "node \"./mocks/concat-json.js\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.0.0-beta.7",
    "@angular/common": "^6.0.0-beta.7",
    "@angular/compiler": "^6.0.0-beta.7",
    "@angular/core": "^6.0.0-beta.7",
    "@angular/forms": "^6.0.0-beta.7",
    "@angular/http": "^6.0.0-beta.7",
    "@angular/platform-browser": "^6.0.0-beta.7",
    "@angular/platform-browser-dynamic": "^6.0.0-beta.7",
    "@angular/router": "^6.0.0-beta.7",
    "@angular/material": "6.0.0-beta.4",
    "angular-auth-oidc-client": "^4.0.3",
    "core-js": "^2.5.3",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
